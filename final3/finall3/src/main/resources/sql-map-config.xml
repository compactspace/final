<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<!-- Alias(객체명) 설정 -->
	<typeAliases>
	<typeAlias type="com.spring.finall.user.ReserveRestVO"  alias="ReserveRestVO" />
	
	<typeAlias type="com.spring.finall.user.ReserveInfoVO"  alias="reserveInfoVO" />
	
	<typeAlias type="com.spring.finall.user.OrderInfoVO"  alias="OrderInfoVO" />	
	<typeAlias type="com.spring.finall.user.PayVO"  alias="payVO" />	                    
	<typeAlias type="com.spring.finall.WorkImgVO"  alias="workimgvo" />
	<typeAlias type="com.spring.finall.user.ReserveVO"  alias="reserveVO" />
	<typeAlias type="com.spring.finall.user.UserVO"  alias="userVO" />
	<typeAlias type="com.spring.finall.user.ProductVO"  alias="productVO" />	
	<typeAlias type="com.spring.finall.user.CartVO"  alias="cartVO" />
	<typeAlias type="com.spring.finall.user.MemberVO"  alias="memberVO" />	
	<typeAlias type="com.spring.finall.user.ReceiptVO"  alias="receiptVO" />
 <typeAlias type="com.spring.finall.BoardVO"  alias="boardVO" /> 
  <typeAlias type="com.spring.finall.BoardinfoVO"  alias="boardinfoVO" /> 
 
<typeAlias type="com.spring.finall.AuthVO"  alias="authnumVO" />
<typeAlias type="com.spring.finall.user.OneDayClassVO"  alias="oneDayClassVO" />

<typeAlias type="com.spring.finall.transaction.TransactionVO"  alias="transactionVO" />

 <typeAlias type="com.spring.finall.security.SecurityUserVO"  alias="securityUserVO" /> 

	</typeAliases>



	<!-- SQL이 작성되는 mapper 파일 위치 등록!
	 MyBatis에서 SQL 매퍼 파일을 등록하는 태그. mapper 태그를 사용하여 등록할 매퍼 파일의 경로를 지정한다.
	 src/main/resources 폴더 내에 위치해 있어야 한다.
	 그래서 mappings 폴더는 src/main/resources 폴더 내에 있다.
	
	 -->
	<mappers>
			<mapper resource="mappings/workimgvo-mapping.xml" />
			<mapper resource="mappings/reservevo-mapping.xml" />
			<mapper resource="mappings/login-mapping.xml" />
			<mapper resource="mappings/productvo-mapping.xml" />			 
		     <mapper resource="mappings/cartvo-mapping.xml"/>
		      <mapper resource="mappings/Receipt-mapping.xml"/>	
		       <mapper resource="mappings/membervo-mapping.xml"/>
		           <mapper resource="mappings/Boardvo-mapping.xml"/> 
		           <mapper resource="mappings/BoardinfoVO-mapping.xml"/> 
		           
		               <mapper resource="mappings/AuthVO-mapping.xml"/>
		               <mapper resource="mappings/OneDayClassVO-mapping.xml"/>
		                   <mapper resource="mappings/TransactionVO-mapping.xml"/>
		                    <mapper resource="mappings/PayVO-mapping.xml"/>
		                    <mapper resource="mappings/OrderInfoVO-mapping.xml"/>
		                    <mapper resource="mappings/ReserveInfoVO-mapping.xml"/>
		                       <mapper resource="mappings/ReserveRestVO-mapping.xml"/>
		                     <mapper resource="mappings/SecurityUserVO-mapping.xml"/> 
		                    	
		                   					
		</mappers>
	
</configuration>